T_0.0 ;
    %load/vec4 v0x600002b3c090_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x600002b3c090_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x11e604800;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600002b3c240_0, 0, 1;
