v0x600002b3c120_0 .net "rst", 0 0, v0x600002b3c240_0;  1 drivers
E_0x60000173c300 .event posedge, v0x600002b3c120_0, v0x600002b3c000_0;
    .scope S_0x11e604db0;
T_0 ;
    %wait E_0x60000173c300;
    %load/vec4 v0x600002b3c120_0;
    %flag_set/vec4 8;
